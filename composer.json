{
    "name": "travi/framework",
    "description": "web framework",
    "version": "0.10.0",
    "authors": [
        {
            "name": "Matt Travi",
            "email": "programmer@travi.org",
            "homepage": "http://travi.org"
        }
    ],
    "require": {
        "smarty/smarty": "3.1.*@stable",
        "fp/lightopenid": "dev-master",
        "html2text/html2text": "1.0.0",
        "lespoilus/spyc": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "behat/behat": "2.5.*",
        "phing/phing": "2.7.0",
        "squizlabs/php_codesniffer": "1.5.2",
        "satooshi/php-coveralls": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Pd": "php/thirdparty/PHP-Dependency/library/",
            "travi\\framework": [
                "php/framework/objects",
                "php/framework/src"
            ]
        }
    }
}