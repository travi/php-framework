{
    "name": "travi/framework",
    "description": "web framework",
    "authors": [
        {
            "name": "Matt Travi",
            "email": "programmer@travi.org",
            "homepage": "http://travi.org"
        }
    ],
    "require": {
        "smarty/smarty": "3.1.*",
        "lightopenid/lightopenid": "0.7.0",
        "html2text/html2text": "1.0.0",
        "mustangostang/spyc": "0.5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "behat/behat": "2.5.*",
        "phing/phing": "2.9.*",
        "phpmd/phpmd": "2.0.0",
        "sebastian/phpcpd": "2.0.1",
        "squizlabs/php_codesniffer": "1.5.3",
        "satooshi/php-coveralls": "0.6.*"
    },
    "autoload": {
        "psr-0": {
            "Pd": "php/thirdparty/PHP-Dependency/library/",
            "travi\\framework": [
                "php/framework/objects",
                "php/framework/src"
